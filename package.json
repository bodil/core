{
    "name": "@bodil/core",
    "version": "0.0.1",
    "description": "My TypeScript standard library",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bodil/core.git"
    },
    "license": "EUPL-1.2+",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js"
            }
        },
        "./assert": {
            "import": {
                "types": "./dist/assert.d.ts",
                "import": "./dist/assert.js"
            }
        },
        "./async": {
            "import": {
                "types": "./dist/async.d.ts",
                "import": "./dist/async.js"
            }
        },
        "./date": {
            "import": {
                "types": "./dist/date.d.ts",
                "import": "./dist/date.js"
            }
        },
        "./event": {
            "import": {
                "types": "./dist/event.d.ts",
                "import": "./dist/event.js"
            }
        },
        "./fun": {
            "import": {
                "types": "./dist/fun.d.ts",
                "import": "./dist/fun.js"
            }
        },
        "./order": {
            "import": {
                "types": "./dist/order.d.ts",
                "import": "./dist/order.js"
            }
        },
        "./types": {
            "import": {
                "types": "./dist/types.d.ts",
                "import": "./dist/types.js"
            }
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsc",
        "test": "vitest --run",
        "lint": "eslint src"
    },
    "dependencies": {
        "@bodil/opt": "^0.2.6"
    },
    "devDependencies": {
        "@bodil/opt-vitest": "^1.0.0",
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.17.0",
        "@typescript-eslint/eslint-plugin": "^8.19.1",
        "@typescript-eslint/parser": "^8.19.1",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jsdoc": "^50.6.1",
        "globals": "^15.14.0",
        "typescript": "^5.7.3",
        "vitest": "^2.1.8"
    }
}
